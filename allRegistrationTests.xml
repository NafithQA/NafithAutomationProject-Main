            <class name="org.nafeth.fleetRegistrationTests.InvestorFleetRegistrationTests"/>
            <class name="org.nafeth.stakeholdersRegistrationTests.DriversStakeholdersRegistrationTests"/>
            <class name="org.nafeth.stakeholdersRegistrationTests.TruckingCompaniesStakeholdersRegistrationTests"/>
            <class name="org.nafeth.stakeholdersRegistrationTests.InvestorsStakeholdersRegistrationTests"/>
